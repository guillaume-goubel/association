<!DOCTYPE html>
<html class="no-js" lang="fr">
	<head>

		<title>Association ARSMélie</title>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
		<meta name="author" content="ThemeZaa">
		<meta name="viewport" content="width=device-width,initial-scale=1.0"/>
		<meta name="description" content="Présentation de l'association arsméli : association sport retraite de lille métropole">
		<!-- favicon icon -->
		<link rel="shortcut icon" href="images/favicon.png">
		<link rel="apple-touch-icon" href="images/apple-touch-icon-57x57.png">
		<link rel="apple-touch-icon" sizes="72x72" href="images/apple-touch-icon-72x72.png">
		<link rel="apple-touch-icon" sizes="114x114" href="images/apple-touch-icon-114x114.png">
		<!-- google fonts preconnect -->
		<link rel="preconnect" href="https://fonts.googleapis.com" crossorigin>
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text><text y=%221.3em%22 x=%220.2em%22 font-size=%2276%22 fill=%22%23fff%22>Plateau</text></svg>">

		{% block stylesheets %}
		{% endblock %}

		{% block baseJavascripts %}
			{% block importmap %}
				{{ importmap(['app']) }}
			{% endblock %}
		{% endblock %}

	</head>

	{% block body %}

		<div class="d-none" id="activityInfosPATH" data-path="{{ path('home_activity_infos')}}"></div>
		<div class="d-none" id="animatorInfosPATH" data-path="{{ path('home_animators_infos')}}"></div>
		<div class="d-none" id="isAdminIsLogged" data-param="{{ (is_granted('ROLE_ADMIN') or is_granted('ROLE_SUPER_ADMIN')) ? true : false }}"></div>

		{% include 'main_partials/parts/_scroll_progress.html.twig' %}
		{% include 'main_partials/notifications/_notyFlashMessage.html.twig' %}
		{% include 'main_partials/modals/_activity_modal.html.twig' %}
		{% include 'main_partials/modals/_animators_modal.html.twig' %}
		{% include 'admin/partials/modals/_disabled_event_modal.html.twig' %}

	{% endblock %}

    <script type="text/javascript" src="{{ asset('js/base/jquery.js') }} " ></script>
    <script type="text/javascript" src="{{ asset('js/base/vendors.min.js') }} "></script>
	<script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js" integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo=" crossorigin=""></script>
    <script src='https://api.mapbox.com/mapbox.js/plugins/leaflet-fullscreen/v1.0.1/Leaflet.fullscreen.min.js'></script>

</html>
